{
  "name": "firebase_test",
  "main": "index.jsx",
  "license": "MIT",
  "scripts": {
    "open": "open -a '/Applications/Google Chrome.app' 'http://localhost:8080/index.html?logging=true'",
    "start": "webpack-dev-server -d --config webpack-dev.config.js",
    "validate": "npm-run-all --parallel jslint validate-webpack:* coverage",
    "validate:ci": "npm-run-all validate coveralls",
    "validate-webpack:dev": "node_modules/webpack-validator/dist/bin/webpack-validator.js webpack-dev.config.js",
    "validate-webpack:prod": "node_modules/webpack-validator/dist/bin/webpack-validator.js webpack-production.config.js",
    "validate-webpack:fastbuild": "node_modules/webpack-validator/dist/bin/webpack-validator.js webpack-fastbuild.config.js",
    "test": "node_modules/mocha/bin/mocha --compilers js:babel-core/register --require ignore-styles --require ./src/__test__/test_helper.js \"src/**/__test__/*_spec@(.js|.jsx)\"",
    "jslint": "./scripts/jslint.sh",
    "csslint": "./scripts/csslint.sh",
    "fastBuild": "webpack --config webpack-fastbuild.config.js --progress --profile --colors",
    "fastBuild:watch": "./node_modules/watch/cli.js 'npm run fastBuild' src",
    "build": "webpack --config webpack-production.config.js --progress --profile --colors",
    "coverage": "node_modules/.bin/babel-node node_modules/.bin/babel-istanbul cover --config .istanbul.yml node_modules/mocha/bin/_mocha -- --require ignore-styles ./src/__test__/test_helper.js \"src/**/__test__/*_spec@(.js|.jsx)\"",
    "coveralls": "npm run-script coverage && ./node_modules/coveralls/bin/coveralls.js < coverage/lcov.info",
    "todo": "grep -rnw 'TODO' src"
  },
